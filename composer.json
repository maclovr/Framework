{
    "name": "nkstamina/framework",
    "description": "The NkStamina Framework",
    "keywords": ["framework", "nkstamina"],
    "homepage": "https://github.com/nkstamina",
    "license": "MIT",
    "authors": [
        {
            "name": "nkstamina",
            "email": "nkstamina@gmail.com"
        }
    ],
    "require": {
        "pimple/pimple": "~3.0",
        "symfony/finder": "2.5.*",
        "symfony/http-foundation": "2.5.*",
        "symfony/http-kernel": "2.5.*",
        "symfony/routing": "2.5.*",
        "symfony/yaml": "2.5.*",
        "symfony/event-dispatcher": "2.5.*",
        "symfony/config": "2.5.*"
    },
    "autoload": {
        "psr-0": {
            "Nkstamina\\Framework\\": ""
        }
    },
    "target-dir": "Nkstamina/Framework",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.0-dev"
        }
    }
}
